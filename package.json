{
  "name": "vitepress-plugin-git",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.8.0",
  "author": "Leo <https://github.com/northword>",
  "license": "MIT",
  "homepage": "https://github.com/northword/vitepress-plugin-git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northword/vitepress-plugin-git.git"
  },
  "bugs": "https://github.com/northword/vitepress-plugin-git/issues",
  "keywords": [
    "vitepress",
    "vite",
    "plugin",
    "git",
    "changelog"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/vscode-icons": "^1.2.18",
    "@iconify/utils": "^2.3.0",
    "@vueuse/core": "^13.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "lint-staged": "^15.5.0",
    "sass": "^1.87.0",
    "simple-git-hooks": "^2.12.1",
    "tsdown": "0.11.0-beta.5",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "unplugin-vue": "^6.2.0",
    "vite": "^6.1.4",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1",
    "vue": "^3.5.13",
    "vue-sfc-transformer": "^0.1.14",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "overrides": {
      "tsdown": "link:../tsdown",
      "vite": "^6.1.4"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
